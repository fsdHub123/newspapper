@import "variable.styl"
* 
  padding 0
  margin 0
  font-family: 'PingFang SC', 'STHeitiSC-Light', 'Helvetica-Light', arial, sans-serif, 'Droid Sans Fallback'
//font-family: Verdana,Arial,SimSun,Helvetica,Georgia,Sans-serif
body, html
  line-height: 1
  user-select: none
  -webkit-tap-highlight-color: transparent
  background: #f9f9f9
  color: #333
  .font-size-small
    font-weight 400
    font-size 1rem
  .bold
    font-weight bold
  ul,li{
    list-style none
  }